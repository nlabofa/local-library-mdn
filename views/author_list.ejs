<%include partials/header%>

<div class="col-sm-10">
<h1><%=title%></h1>
<ul>
<%if(author_list != null){%>
<%author_list.forEach(function(author){%>
  <li><a href="<%=author.url%>"><%=author.name%></a> (<%=author.date_of_birth_formatted%> - <%=author.date_of_death_formatted%>)</li>
<%})%>
<%} else{%>
  <li>There are no authors</li>
<%}%>
</div>
<%include partials/footer%>
